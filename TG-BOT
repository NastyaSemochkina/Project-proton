import telebot
import sqlite3
import requests
import tkinter as tk
from tkinter import PhotoImage

from telebot.types import Message
from telebot import types

client = telebot.TeleBot('7408352753:AAFMCMjBkPXugcXhpiLg4vOBKnXvNnhOV9c')


@client.message_handler(commands=['start'])
def welcome(message, points=None):
    connect = sqlite3.connect('users.db')
    cursor = connect.cursor()

    cursor.execute("""CREATE TABLE IF NOT EXISTS login_id(
        id INTEGER, 
        points INTEGER DEFAULT 0
    )""")

    connect.commit()

    people_id = message.chat.id
    cursor.execute("SELECT id FROM login_id WHERE id = ?", (people_id,))
    data = cursor.fetchone()

    if data is None:
        cursor.execute("INSERT INTO login_id VALUES(?, ?);", (message.chat.id, points))
        connect.commit()
    else:
        client.send_message(message.chat.id, '–¢–∞–∫–æ–π –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å —É–∂–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç!')
    markup_inline = types.InlineKeyboardMarkup()
    item_1 = types.InlineKeyboardButton('–ù–∞—á–Ω—ë–º —É–≤–ª–µ–∫–∞—Ç–µ–ª—å–Ω–æ–µ –æ–±—É—á–µ–Ω–∏–µ!', callback_data='starting')
    markup_inline.add(item_1)
    client.send_message(message.chat.id, f'–ü—Ä–∏–≤–µ—Ç!\n–Ø –ø–æ–º–æ–≥—É —Ç–µ–±–µ —Ä–∞–∑–≤–∏—Ç—å –≥–∏–±–∫–æ—Å—Ç—å —É–º–∞, –Ω–∞–±–ª—é–¥–∞—Ç–µ–ª—å–Ω–æ—Å—Ç—å, –ø–∞–º—è—Ç—å –∏ —É–ª—É—á—à–∏—Ç—å –∑–Ω–∞–Ω–∏—è –º–∞—Ç–µ–º–∞—Ç–∏–∫–∏:)\n–í—ã–ø–æ–ª–Ω—è–π –∑–∞–¥–∞–Ω–∏—è, –ø—Ä–æ—Ö–æ–¥–∏ —É—Ä–æ–∫–∏ –∏ –ø–æ–ª—É—á–∞–π –ø–µ—á–µ–Ω—å–∫–∏üç™', reply_markup=markup_inline)



